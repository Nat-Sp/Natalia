import math
def is_square(n):
    if n>=0:
        if int(math.sqrt(n))*int(math.sqrt(n))==n:
            return True
        else:
            return False # fix me
    else:
        return False
print(is_square(-1))
print(is_square( 0))
print(is_square( 3))
print(is_square( 4))
print(is_square(25))
print(is_square(26))
